{
  "name": "doc",
  "version": "0.0.6",
  "description": "A Quasar Project",
  "productName": "Doc",
  "author": "q.chen.work <q.chen.work@outlook.com>",
  "private": true,
  "scripts": {
    "preview": "quasar serve",
    "lint": "eslint --ext .js,.ts,.vue ./",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "dev:electron": "quasar dev -m electron",
    "build": "pnpm run build:spa",
    "build:mac": "quasar build -m electron -T mac",
    "build:win": "quasar build -m electron -T win",
    "build:spa": "pnpm run fix-preload && quasar build -m spa",
    "fix-preload": "node ./fix-preload.js"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "axios": "^1.6.2",
