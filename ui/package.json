    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.19.2",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3",
    "unocss": "0.55.7",
    "unplugin-auto-import": "^0.17.2",
    "unplugin-vue-components": "^0.26.0"
  },
